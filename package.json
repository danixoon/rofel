{
  "name": "rofel-d",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "debug": "node --inspect=5858 -r ts-node/register ./src/index.ts",
    "start": "node ./dist/index.js",
    "build": "tsc",
    "watch": "nodemon"
  },
  "nodemonConfig": {
    "ignore": [
      "**/*.test.ts",
      "**/*.spec.ts",
      ".git",
      "node_modules"
    ],
    "watch": [
      "src/**/*"
    ],
    "exec": "yarn start",
    "ext": "ts"
  },
  "dependencies": {
    "@discordjs/opus": "^0.3.2",
    "@google-cloud/text-to-speech": "^3.1.0",
    "@types/node": "^12.7.1",
    "@types/yargs": "^15.0.5",
    "axios": "^0.20.0",
    "discord.js": "^12.3.1",
    "dotenv": "^8.0.0",
    "ffmpeg-static": "^2.5.0",
    "random-reddit": "^1.1.0",
    "ts-node": "^8.3.0",
    "typescript": "^4.0.2",
    "util": "^0.12.1",
    "yargs": "^15.4.1",
    "ytdl-core": "^3.2.2"
  }
}
